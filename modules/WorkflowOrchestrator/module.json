{
  "name": "WorkflowOrchestrator",
  "version": "1.0.0",
  "description": "Orchestrates workflow execution and manages agent sequence",
  "category": "Workflow Management",
  "project": "AIDeveloper",
  "tags": [
    "orchestration",
    "workflows",
    "management"
  ],
  "scripts": {
    "install": "npm install --include=dev",
    "build": "npm run build",
    "typecheck": "npm run typecheck",
    "frontend-install": "cd frontend && npm install",
    "frontend-dev": "cd frontend && npm run dev",
    "frontend-build": "cd frontend && npm run build"
  },
  "pages": [
    {
      "path": "/workflows",
      "component": "Workflows",
      "label": "Workflows",
      "icon": "Activity",
      "navOrder": 2
    },
    {
      "path": "/workflows/:id",
      "component": "WorkflowDetail",
      "label": "Workflow Detail",
      "navOrder": 0
    },
    {
      "path": "/workflow-testing",
      "component": "WorkflowTesting",
      "label": "Agent Testing",
      "icon": "TestTube",
      "navOrder": 3
    }
  ],
  "envVars": [
    {
      "key": "OPENROUTER_API_KEY",
      "description": "OpenRouter API key",
      "required": true,
      "type": "string",
      "secret": true,
      "modulePrefix": "WORKFLOW"
    },
    {
      "key": "OPENROUTER_MODEL_ORCHESTRATOR",
      "description": "OpenRouter model for orchestration",
      "required": false,
      "defaultValue": "anthropic/claude-3.5-sonnet",
      "type": "string",
      "modulePrefix": "WORKFLOW"
    }
  ],
  "frontend": {
    "port": 5175,
    "enabled": true
  }
}