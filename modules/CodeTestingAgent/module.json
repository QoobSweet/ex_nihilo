{
  "name": "CodeTestingAgent",
  "version": "1.0.0",
  "description": "Code testing agent with read, write, and execute access to create and run tests",
  "category": "AI Agents",
  "project": "AIDeveloper",
  "tags": [
    "ai",
    "testing",
    "qa",
    "agent"
  ],
  "scripts": {
    "install": "npm install",
    "build": "npm run build",
    "typecheck": "npm run typecheck"
  },
  "envVars": [
    {
      "key": "OPENROUTER_API_KEY",
      "description": "OpenRouter API key",
      "required": true,
      "type": "string",
      "secret": true,
      "modulePrefix": "CODETESTING"
    },
    {
      "key": "OPENROUTER_MODEL_TESTING",
      "description": "OpenRouter model for testing",
      "required": false,
      "defaultValue": "anthropic/claude-3.5-haiku",
      "type": "string",
      "modulePrefix": "CODETESTING"
    }
  ]
}