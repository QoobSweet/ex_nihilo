{
  "name": "aideveloper-frontend",
  "version": "1.0.0",
  "description": "Frontend for AIDeveloper - AI-powered development workflow orchestration",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
