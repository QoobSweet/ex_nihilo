# Node Environment
NODE_ENV=development
PORT=3000

# OpenRouter API Configuration
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here
OPENROUTER_MODEL_PLANNING=x-ai/grok-code-fast-1
OPENROUTER_MODEL_CODING=x-ai/grok-code-fast-1
OPENROUTER_MODEL_TESTING=x-ai/grok-code-fast-1
OPENROUTER_MODEL_REVIEW=x-ai/grok-code-fast-1
OPENROUTER_MODEL_DOCS=x-ai/grok-code-fast-1

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your-database-password
DB_NAME=aideveloper

# Redis Configuration (for Bull queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Webhook Security
WEBHOOK_SECRET_GITHUB=your-github-webhook-secret
WEBHOOK_SECRET_GITLAB=your-gitlab-webhook-secret
WEBHOOK_SECRET_CUSTOM=your-custom-webhook-secret

# Git Configuration
GIT_USER_NAME=AIDeveloper Bot
GIT_USER_EMAIL=aideveloper@bot.local
GIT_DEFAULT_BRANCH=develop

# GitHub Configuration (for PR creation)
# Required for automatic PR creation when workflows complete
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_OWNER=YourGitHubUsername
GITHUB_REPO=your-repository-name

# Agent Configuration
MAX_CONCURRENT_AGENTS=5
AGENT_TIMEOUT_MS=300000
MAX_RETRY_ATTEMPTS=3

# Workspace Configuration
WORKSPACE_ROOT=/home/kevin/Home/ex_nihilo

# Logging
LOG_LEVEL=info
LOG_DIR=./logs
